    //- Javascript for individual accordion feature
    script.
      var acc = document.getElementsByClassName("accordion");
      var i;
      for (i = 0; i < acc.length; i++) {
      acc[i].addEventListener("click", function() {
      /* Toggle between adding and removing the "active" class,
      to highlight the button that controls the panel */
      this.classList.toggle("active");

      /* Toggle between hiding and showing the active panel */
      var panel = this.nextElementSibling;
      if (panel.style.display === "block") {
          panel.style.display = "none";
      } else {
          panel.style.display = "block";
      }
      });
      }
     //- Javascript for expand/collapse all feature
            
    
        function expandAll() {
          //- var currentScrollPos = $(window).scrollTop();
          //- console.log(currentScrollPos)
          Array.from(document.getElementsByClassName("panel")).forEach(function(element) {
          element.style.display = "block";
            })      
          Array.from(document.getElementsByClassName("accordion")).forEach(function(element) {
          element.className += " active";
          //- $(window).scrollTop(currentScrollPos);
            })       
        }      

        function collapseAll() {
          //- var currentScrollPos = $(window).scrollTop();
          Array.from(document.getElementsByClassName("panel")).forEach(function(element) {
          element.style.display = "none";
            }) 
          Array.from(document.getElementsByClassName("accordion")).forEach(function(element) {
          element.classList.remove("active");
          //- $(window).scrollTop(currentScrollPos);
            }) }
      

