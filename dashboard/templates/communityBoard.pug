                td.hpdviols #{violation.zipcode}
                td.tc.hpdviols #{violation.residentialunits}
                td.tc.gray.hpdviols #{violation.uc2007} 
                td.tc.hpdviols #{violation.uc2019}
                td.tc.hpdviols #{violation.class_a}
                td.tc.hpdviols #{violation.class_b}
                td.tc.hpdviols #{violation.class_c}
                td.tc.hpdviols #{violation.total}
                td.f7.hpdviols <a href="http://whoownswhat.justfix.nyc/bbl/#{violation.borocode}/#{violation.block}/#{violation.lot}" target="_blank">#{violation.owner}</a>
                td.no-print.hpdviols 
                  <a href="#{violation.hpdlink}" target="_blank">(HPD)</a> 
                  <a href="http://a810-bisweb.nyc.gov/bisweb/PropertyProfileOverviewServlet?boro=#{violation.borocode}&block=#{violation.block}&lot=#{violation.lot}" target="_blank">(DOB)</a>
